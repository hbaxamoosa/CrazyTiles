<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/gameFragment2x2_destination">

    <fragment
        android:id="@+id/gameFragment2x2_destination"
        android:name="com.boxofm.crazytiles.game.GameFragment2x2"
        android:label="@string/GameFragment2x2"
        tools:layout="@layout/fragment_game_2x2">
        <action
            android:id="@+id/action_gameFragment2x2_to_scoreFragment"
            app:destination="@id/score_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_gameFragment2x2_to_gameFragment3x3"
            app:destination="@id/gameFragment3x3_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_gameFragment2x2_to_gameFragment4x4"
            app:destination="@id/gameFragment4x4_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_gameFragment2x2_to_infoFragment"
            app:destination="@id/infoFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/gameFragment3x3_destination"
        android:name="com.boxofm.crazytiles.game.GameFragment3x3"
        android:label="@string/GameFragment3x3"
        tools:layout="@layout/fragment_game_3x3">
        <action
            android:id="@+id/action_gameFragment3x3_to_scoreFragment"
            app:destination="@id/score_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_gameFragment3x3_to_gameFragment2x2"
            app:destination="@id/gameFragment2x2_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_gameFragment3x3_to_gameFragment4x4"
            app:destination="@id/gameFragment4x4_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_gameFragment3x3_to_infoFragment"
            app:destination="@id/infoFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/gameFragment3x3_destination" />
    </fragment>
    <fragment
        android:id="@+id/gameFragment4x4_destination"
        android:name="com.boxofm.crazytiles.game.GameFragment4x4"
        android:label="@string/GameFragment4x4"
        tools:layout="@layout/fragment_game_4x4">
        <action
            android:id="@+id/action_gameFragment4x4_to_scoreFragment"
            app:destination="@id/score_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:launchSingleTop="true"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_gameFragment4x4_to_gameFragment2x2"
            app:destination="@id/gameFragment2x2_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_gameFragment4x4_to_gameFragment3x3"
            app:destination="@id/gameFragment3x3_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <action
            android:id="@+id/action_gameFragment4x4_to_infoFragment"
            app:destination="@id/infoFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/score_destination"
        android:name="com.boxofm.crazytiles.score.ScoreFragment"
        android:label="@string/ScoreFragment"
        tools:layout="@layout/score_fragment">
        <action
            android:id="@+id/action_scoreFragment_to_gameFragment"
            app:destination="@id/gameFragment2x2_destination"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
        <argument
            android:name="winner"
            app:argType="boolean"
            android:defaultValue="false" />
        <argument
            android:name="score"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/infoFragment"
        android:name="com.boxofm.crazytiles.info.InfoFragment"
        android:label="@string/InfoFragment"
        tools:layout="@layout/fragment_info" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.boxofm.crazytiles.settings.SettingsFragment"
        android:label="@string/SettingsFragment"
        tools:layout="@layout/fragment_settings" />
</navigation>